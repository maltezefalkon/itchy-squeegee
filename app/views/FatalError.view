<!DOCTYPE html>
<html lang="en">
<head>
	<%=headerTags%>
    <title><%=pageTitle%></title>
    <style type="text/css">
    </style>
</head>
<body ng-app="safeApp" ng-controller="controller">
    <div class="container">
        <%=pageHeader%>
		<%=pageMasthead%>
        <div class="row">
            <div class="col-xs-12">
                <div class="text-danger lead">
				<%=message%>
                </div>
            </div>
        </div>
        <div class="row">&nbsp;</div>
        <div class="row">
            <div class="col-sm-offset-2 col-sm-8 col-xs-12">
                <div class="btn-toolbar">
                    <button type="button" class="btn btn-lg btn-primary btn-block btn-default" onclick="window.location = '/'">
                        Return to Home Page
                    </button>
                </div>
            </div>
        </div>
        <%=pageFooter%>
    </div>
</body>
</html>